{
  "name": "honeycoin-audit-trail",
  "version": "1.0.0",
  "description": "An SQL-based audit-trail service built with Node.js, TypeScript, and Express.",
  "main": "src/server.ts",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "migrate": "psql -U <username> -d <database> -f src/db/migrations/001_create_audit_entries_table.sql",
    "seed": "ts-node scripts/seed-audit-data.ts",
    "test": "jest"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.0.0",
    "jest": "^27.0.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "audit",
    "trail",
    "service",
    "nodejs",
    "typescript",
    "express"
  ],
  "author": "Your Name",
  "license": "MIT"
}
